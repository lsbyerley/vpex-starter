{
  "name": "vpex-starter",
  "version": "1.0.0",
  "description": "Vue, Parcel, Express starter",
  "main": "main.js",
  "scripts": {
    "client:dev": "parcel index.html",
    "server:dev": "nodemon ./server/app.js --watch ./server",
    "dev": "npm-run-all -p client:dev server:dev",
    "client:prod": "parcel build index.html",
    "server:prod": "node ./server/app.js",
    "build": "npm-run-all client:prod server:prod",
    "prebuild": "rm -rf dist"
  },
  "author": "lsbyerley@gmail.com",
  "license": "ISC",
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "axios": "^0.19.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.2",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@tailwindcss/ui": "^0.1.3",
    "autoprefixer": "^9.7.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@vue/component-compiler-utils": "^3.1.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.26.2",
    "tailwindcss": "^1.2.0",
    "vue-hot-reload-api": "^2.3.4",
    "vue-template-compiler": "^2.6.11"
  }
}
